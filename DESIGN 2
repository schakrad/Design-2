class MyQueue {

   private Stack<Integer> input = new Stack<>();
   private Stack<Integer> output = new Stack<>();
    int front;
    
    public void push(int x) {
        input.push(x);
    }
    
    
    public void shift(){
        
        if (output.isEmpty())
        {
            while (!input.isEmpty())
                output.push(input.pop());
        }
          
    }

    public void pop() {
      
    shift();    
    output.pop();
    }

    public int peek() {
        shift();
        return (output.peek());
       
    }

    public boolean empty() {
        return input.empty() && output.empty();
    }
}
